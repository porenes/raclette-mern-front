Id) => {\n  // ! We do not use likerId for the API call, as the user is authenticated\n  return (dispatch) => {\n    return axios({\n      method: \"put\",\n      url: `${process.env.REACT_APP_RACLETTE_API_URL}post/unlike/${postId}`,\n      withCredentials: true,\n      headers: {\n        Authorization: authHeader,\n      },\n    }).then((res) => {\n      dispatch({ type: UNLIKE_POST, payload: { postId, likerId } });\n    });\n  };\n};\n","/Users/patxol/Projects/raclette-mern-front/src/reducers/posts.reducer.js",[],"/Users/patxol/Projects/raclette-mern-front/src/components/Post/Card.js",["132"],"/Users/patxol/Projects/raclette-mern-front/src/Utils.js",[],"/Users/patxol/Projects/raclette-mern-front/src/reducers/userList.reducer.js",[],"/Users/patxol/Projects/raclette-mern-front/src/components/Post/CreatePost.js",[],"/Users/patxol/Projects/raclette-mern-front/src/components/Post/DeletePost.js",[],"/Users/patxol/Projects/raclette-mern-front/src/components/Post/CheesyLike.js",[],"/Users/patxol/Projects/raclette-mern-front/src/components/Connoisseur/Me.js",[],"/Users/patxol/Projects/raclette-mern-front/src/reducers/parties.reducer.js",[],"/Users/patxol/Projects/raclette-mern-front/src/actions/party.action.js",[],"/Users/patxol/Projects/raclette-mern-front/src/components/Party/Timeline.js",[],"/Users/patxol/Projects/raclette-mern-front/src/components/Party/Card.js",["133"],"/Users/patxol/Projects/raclette-mern-front/src/components/Party/CreateParty.js",[],{"ruleId":"134","replacedBy":"135"},{"ruleId":"136","replacedBy":"137"},{"ruleId":"138","severity":1,"message":"139","line":2,"column":10,"nodeType":"140","messageId":"141","endLine":2,"endColumn":20},{"ruleId":"138","severity":1,"message":"142","line":3,"column":10,"nodeType":"140","messageId":"141","endLine":3,"endColumn":20},{"ruleId":"138","severity":1,"message":"143","line":1,"column":10,"nodeType":"140","messageId":"141","endLine":1,"endColumn":25},{"ruleId":"138","severity":1,"message":"143","line":3,"column":10,